    "gauge_configs = Nf211.objects.all()"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "keys = (\"nx\", \"nt\", \"gaugeaction\", \"light\", \"strange\", \"charm\")\n",
    "for distinct in Nf211.objects.values_list(*keys).distinct():\n",
    "    query = {key: val for key, val in zip(keys, distinct)}\n",
    "    nfs = Nf211.objects.filter(**query)\n",
    "    config = nfs.first()\n",
    "    label = f\"Baryon2pt->{config.long_tag}\"\n",
    "    ensemble, created = Ensemble.objects.get_or_create(label=label)   \n",
    "    ensemble.configurations.add(*nfs)\n",
    "    ensemble.save()"
   ]
